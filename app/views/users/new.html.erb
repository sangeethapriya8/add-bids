<h1>Registeration Form</h1>

<%= form_for @user do |f| %>
	<%= f.label :fullname %>
	<%= f.text_field :fullname %><br>
	<%= f.label :role %>
	<%= f.select :role,["seller","buyer"] %><br>
	<%= f.label :username %>
	<%= f.text_field :username %><br>
	<%= f.label :password %>
	<%= f.password_field :password %><br>
	<%= f.submit "Create Account" %>
<% end %>

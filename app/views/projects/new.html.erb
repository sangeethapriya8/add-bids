<h1>Project Details</h1>

<%#= form_for (@project,url="user_projects_path(@user)") do |f| %>
<%= form_for @project, url: user_projects_path(@user), method: :post do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %><br>
	<%= f.label :skill %>
	<%= f.text_field :skill %><br>
	<%= f.label :state %>
	<%= f.text_field :state %><br>
	<%= f.label :city %>
	<%= f.text_field :city %><br>
	<%= f.submit "Create Project"%>
<% end %>